<script>



   const checkInterval = setInterval(() => {
const serviceQuestions = document.querySelectorAll('.services_question');
if (serviceQuestions.length > 0) {
 clearInterval(checkInterval); // Stop checking once the element is found

 serviceQuestions.forEach((svc) => {
   const el = svc.nextElementSibling;
   el.style.height = "0px";

   svc.addEventListener('click', () => {
     const answer = svc.nextElementSibling;
     if (answer.style.height === "0px" || !answer.style.height) {
       answer.style.height = answer.scrollHeight + "px";
     } else {
       answer.style.height = "0px";
     }
   });
 });
}
}, 100);



</script>
<script>
   document.addEventListener("DOMContentLoaded", function () {
       let container = document.querySelector(".post-card-container");
       let wrapper = document.querySelector(".post-card-container .post-card-wrap");
   
       function cloneItems() {
           let clone = wrapper.cloneNode(true);
           container.appendChild(clone);
       }
   
       cloneItems();
   });
   </script>





